{
  "name": "EagleDelta2-autofs",
  "version": "1.0.1",
  "author": "David Hollinger",
  "summary": "Module for installing and managing autofs",
  "license": "Apache-2.0",
  "source": "https://github.com/EagleDelta2/autofs-puppet.git",
  "project_page": "https://github.com/EagleDelta2/autofs-puppet",
  "issues_url": "https://github.com/EagleDelta2/autofs-puppet/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "20",
        "21"
      ]
    }],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.0.0"
    },{
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.1"
      }
    ]
}
